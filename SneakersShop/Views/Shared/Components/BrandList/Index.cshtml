@model IEnumerable<SneakersShop.Models.Brand>
@using SneakersShop.Models.ViewModels

	<div class="col-sm-12">
		<div class="side border mb-1">
			<h3>Brand</h3>
			@foreach (var item in Model)
			{
				<ul>
				<li><a href="/Products/ProductsByBrand/?brandId=@item.BrandId">@item.BrandName</a></li>
				</ul>
			}
		</div>
	</div>

